pipeline{
 
    /* A decalarative pipeline*/
    agent any
 
    stages{
 
        stage('Initialize'){
 
            steps{
			
			echo "Initialize the code file"
 
  
            }
        }
 
        stage('Build'){
 
            steps{
			  echo "Hello world"
 
 
            }
        }
 
       
 
        stage('Deploy to Staging'){
            /*Agent selection could go here as well*/
            steps{
			echo "Hello world"
 
 
            }
 
        }
    }
}

